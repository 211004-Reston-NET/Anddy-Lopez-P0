@{
}

@model IEnumerable<P0WebUI.Models.StoreFrontVM>

<h1 class="display-4">List of Store Fronts</h1>

<a class="btn btn-primary text-white" asp-controller="StoreFront" asp-action="Create">Add Store Front</a>
<br />
<br />
<table class="table table-striped table-hover">
    <tr>
        <th>Name</th>
        <th>Address</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelitem => item.SName)</td>
            <td>@Html.DisplayFor(modelitem => item.SAddress)</td>
        </tr>
    }
</table>